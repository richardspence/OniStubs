                break;
            case "op_GreaterThanOrEqual":
                return "operator >=";
                break;
            case "op_LessThan":
                return "operator <";
                break;
            case "op_GreaterThan":
                return "operator >";
                break;
             case "op_Addition":
                return "operator +";
                break;
            case "op_Subtraction":
                return "operator -";
                break;
            case "op_Multiply":
                return "operator *";
                break;
            case "op_Division":
                return "operator /";
                break;
        }
        return "UNK:"+name;
    }

